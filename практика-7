#include <stdio.h>

int max(int a, int b) {
    return (a > b) ? a : b;
}

int main() {
    int x, y; //создаем переменные x,y
    printf("Введите 2 числа\n"); //ввод чисел для x, y
    scanf("%d %d", &x, &y); //присваиваем данные 
    printf("Максимум из %d и %d: %d\n", x, y, max(x, y)); //вывод макс числа 
    return 0;
}
