#include <stdio.h>

int main() {
    // Создаем массив из 3 строк
    char colors[3][10] = {"green", "red", "blue"};

    // Меняем первый элемент (красный) на желтый
    strcpy(colors[0], "white");

    // Выводим массив на экран
    for (int i = 0; i < 3; i++) {
        printf("%s\n", colors[i]);
    }

    return 0;
}
